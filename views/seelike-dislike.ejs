<!DOCTYPE html>
<html>
<head>
  <title>Student Images</title>
  <style>
    *{
      margin: 0px;
    }
    table {
      width: 90%;
      margin: auto;
      border-collapse: collapse;
      margin-top: 50px;
    }
    tr th{
      font-size: 20px;
    }
    
    th, td {
      padding: 4px;
      border-bottom: 2px solid #ddd;
    }
    td{
        text-align: center;
    }
  
    h1{
        text-align: center;
        margin-top: 30px;
    }

    tr th{
            padding: 10px;
            font-size: 25px;
            border: 2px solid grey;
        }
        td{
            text-align: center;
            border: 2px solid grey;
        }
    tr:nth-child(even) {
        background-color: #dddddd;
    }

  </style>
</head>
<body>
    <h1>See like dislike</h1>
    <table>
      <thead>
        <tr>
          <th>ID</th>
          <th>User ID</th>
          <th>postid</th>
          <th>Like</th>
          <th>DisLike</th>
        </tr>
      </thead>
      <tbody>
        <% posts.forEach(function(post) { %>
          <tr>
            <td><%= post.id %></td>
            <td><%= post.userId %></td>
            <td><%= post.postid %></td>
            <td><%= post.Like %></td>
            <td><%= post.DisLike %></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
    
</body>